{
    "name": "eggd_apheleia_v1.0.0",
    "title": "eggd_apheleia_v1.0.0",
    "summary": "Predicts the possible presence of KMT2A-PTDs using mean exon coverage",
    "dxapi": "1.0.0",
    "version": "1.0.0",
    "inputSpec": [
      {
        "name": "exon_stats",
        "label": "Exon statistics file",
        "help": "Exon-level coverage file generated by Athena",
        "class": "file",
        "patterns": ["*_exon_stats.tsv"],
        "optional": false
      }
    ],
    "outputSpec": [
      {
        "name": "apheleia_tsv",
        "label": "Apheleia TSV",
        "help": "TSV file containing Apheleia output",
        "class": "file"
      }
    ],
    "runSpec": {
      "execDepends": [],
      "distribution" : "Ubuntu",
      "release":"20.04",
      "version":"0",
      "file": "src/code.sh",
      "interpreter": "bash",
      "timeoutPolicy": {
        "*": {
          "hours": 1
        }
      },
      "assetDepends": [
      ]
    },
    "developers":[
      "org-emee_1"
    ],
    "authorizedUsers": [
      "org-emee_1"
    ],
    "access": {
      "allProjects":"UPLOAD"
    },
    "regionalOptions": {
      "aws:eu-central-1": {
        "systemRequirements": {
          "main": {
            "instanceType": "mem1_ssd1_x2"
          }
        }
      }
    }
  }
